<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:util="http://www.springframework.org/schema/util"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:mvc="http://www.springframework.org/schema/mvc"
	xsi:schemaLocation="http://www.springframework.org/schema/mvc http://www.springframework.org/schema/mvc/spring-mvc-4.1.xsd
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
                http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.1.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <bean id="storageServiceSwift" class="eea.eprtr.cms.dao.StorageServiceSwift">
        <property name="swiftAuthUrl" value="${swift.authurl}" />
        <property name="swiftTenantId" value="${swift.tenantid}" />
        <property name="swiftUsername" value="${swift.username}" />
        <property name="swiftPassword" value="${swift.password}" />
        <property name="swiftContainer" value="${swift.container}" />
        <property name="swiftMock" value="${swift.mock}" />
    </bean>

    <bean id="storageServiceFiles" class="eea.eprtr.cms.dao.StorageServiceFiles">
        <property name="storageDir" value="${storage.dir}" />
    </bean>

</beans>
